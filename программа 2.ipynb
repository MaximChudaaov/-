    "import socket\n",
    "\n",
    "s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)\n",
    "s.connect(('localhost', 12349))\n",
    "\n",
    "while True:\n",
    "    num = input()\n",
    "    s.send(num.encode())\n",
    "    if num == 'q':\n",
    "        break\n",
    "\n",
    "s.close()"
   ]
  }
